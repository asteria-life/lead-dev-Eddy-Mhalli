    "body-parser": "1.20.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "envalid": "8.0.0",
    "lodash": "4.17.21",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
